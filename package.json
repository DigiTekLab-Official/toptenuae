{
  "name": "toptenuae",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20 <=20",
    "pnpm": ">=8"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "pages:build": "opennextjs-cloudflare build",
    "pages:deploy": "wrangler pages deploy .open-next --project-name=toptenuae",
    "pages:deploy:production": "wrangler pages deploy .open-next --project-name=toptenuae --branch=main",
    "preview": "opennextjs-cloudflare preview",
    "deploy": "pnpm run pages:build && pnpm run pages:deploy",
    "deploy:prod": "pnpm run pages:build && pnpm run pages:deploy:production",
    "start": "next start",
    "lint": "eslint"
  },
  "dependencies": {
    "@formspree/react": "^3.0.0",
    "@next/third-parties": "^16.1.1",
    "@portabletext/react": "^5.0.0",
    "@sanity/image-url": "^2.0.2",
    "aws4": "^1.13.2",
    "clsx": "^2.1.1",
    "lucide-react": "^0.555.0",
    "next": "16.0.10",
    "next-sanity": "11.6.12",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-syntax-highlighter": "^16.1.0",
    "resend": "^6.6.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@opennextjs/cloudflare": "^1.14.7",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/aws4": "^1.11.6",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-syntax-highlighter": "^15.5.13",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "tailwindcss": "^4.1.18",
    "typescript": "^5",
    "wrangler": "^4.54.0"
  },
  "pnpm": {
    "overrides": {
      "prismjs": ">=1.30.0"
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp",
      "unrs-resolver",
      "workerd"
    ]
  }
}
